{
  "name": "stable_api",
  "version": "1.0.0",
  "description": "stable_api",
  "scripts": {
    "build": "babel src --out-dir dist",
    "start": "NODE_ENV=PROD node ./dist/bin/www.js",
    "dev": "NODE_ENV=DEV  nodemon --ignore dist/ --ignore test/ --exec babel-node ./src/bin/www.js",
    "lint": "eslint .",
    "prettier": "prettier . --check",
    "prettier:fix": "npm run prettier -- --write",
    "test": "NODE_ENV=TEST mocha . --require @babel/register --exit",
    "prepare": "husky install",
    "db:migrate:dev": "NODE_ENV=DEV npx sequelize-cli db:migrate",
    "db:rollback:dev": "NODE_ENV=DEV npx sequelize-cli db:migrate:undo:all",
    "db:seed:dev": "NODE_ENV=DEV npx sequelize-cli db:seed:all",
    "db:unseed:dev": "NODE_ENV=DEV npx sequelize-cli db:seed:undo:all",
    "db:migrate:test": "NODE_ENV=TEST npx sequelize-cli db:migrate",
    "db:rollback:test": "NODE_ENV=TEST npx sequelize-cli db:migrate:undo:all",
    "db:seed:test": "NODE_ENV=TEST npx sequelize-cli db:seed:all",
    "db:unseed:test": "NODE_ENV=TEST npx sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olivierHuvelle/stable_api.git"
  },
  "author": "Olivier Huvelle",
  "bugs": {
    "url": "https://github.com/olivierHuvelle/stable_api/issues"
  },
  "homepage": "https://github.com/olivierHuvelle/stable_api#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "enum": "^3.0.4",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "express-winston": "^4.2.0",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "i18next": "^22.5.1",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.7",
    "sequelize": "^6.32.0",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@faker-js/faker": "^8.0.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "eslint": "^8.42.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.1"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
